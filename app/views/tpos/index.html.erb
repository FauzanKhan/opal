<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, Tpo.find_by(email: user.email) || Student.find_by(email: user.email) %>
      <%= user.college %>
      <%= user.user_type %>
      <% if current_user.admin? %>
    |	 <%= link_to "delete", user, method: :delete, data: { confirm: "You sure?" } %>
  	  <% end %>
    </li>
  <% end %>
</ul>